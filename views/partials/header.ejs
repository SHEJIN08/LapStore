        <script>
    // This script checks if the page is being loaded from the browser's "Back-Forward Cache"
    window.addEventListener('pageshow', function (event) {
        // 'event.persisted' is true if the page was restored from the bfcache
        if (event.persisted) {
            console.log("Page loaded from cache. Forcing reload...");
            window.location.reload();
        }
    });
</script>